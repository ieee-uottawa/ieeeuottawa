{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "max-len": [
      "warn",
      {
        "code": 150
      }
    ],
    "linebreak-style": "off",
    "object-curly-newline": "off",
    "react/jsx-one-expression-per-line": "off",
    "implicit-arrow-linebreak": "off",
    "comma-dangle": "off"
  }
}
